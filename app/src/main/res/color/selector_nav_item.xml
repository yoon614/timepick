<?xml version="1.0" encoding="utf-8"?>
<!-- 하단 네비게이션 아이템 상태별 색상 셀렉터 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 선택된 상태 (현재 화면) -->
    <item
        android:color="#51555C"
        android:state_checked="true" />

    <!-- 선택되지 않은 상태 -->
